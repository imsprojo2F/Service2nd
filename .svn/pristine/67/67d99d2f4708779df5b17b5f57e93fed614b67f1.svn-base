#loading-mask {
	background: #DEE1E2;
	height: 100%;
	left: 0;
	opacity: 1;
	filter: alpha(opacity = 100);
	position: fixed;
	text-align: center;
	top: 0;
	width: 100%;
	z-index: 999999999;
	display: none;
}

.loading-img {
	height: 100%;
	left: 0;
	position: fixed;
	top: 20%;
	width: 100%;
}